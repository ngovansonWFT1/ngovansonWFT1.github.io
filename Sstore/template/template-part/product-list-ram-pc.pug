                                    img(src="./../images/product-list/grid-active.svg", alt="grid-icon")
                                .sort-product-by-brand
                                    .dropdown
                                        button#dropdownMenuButton.btn.btn-secondary.dropdown-toggle(type='button' data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                                            | Mặc định
                                        .dropdown-menu(aria-labelledby='dropdownMenuButton')
                                            a.dropdown-item(href='#' data-sort-by= "all" data-filter="all") Mặc định
                                            a.dropdown-item(href='#' data-sort-by="name" data-filter="ASC") A - Z
                                            a.dropdown-item(href='#' data-sort-by="name" data-filter="DESC") Z - A
                                            a.dropdown-item(href='#' data-sort-by="price" data-filter="ASC") Giá tăng dần
                                            a.dropdown-item(href='#' data-sort-by="price" data-filter="DESC") Giá giảm dần
