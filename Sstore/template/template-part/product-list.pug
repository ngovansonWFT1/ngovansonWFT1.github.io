extends layout.pug

block header 
    title Sstore - Danh sách sản phẩm

block content
    section.product-list
        .container
            .row
                .col-md-3
                    .filter-product

                        .filter-by-brand.filter-prd
                            .title-brand
                                h4 Thương hiệu
                            .form-check
                                label
                                    input(type='radio' name='brand' value="name" data-filter="Samsung")/ Samsung
                                    span.checkmark
                                label
                                    input(type='radio' name='brand' value="name" data-filter="Crucial")/ Crucial
                                    span.checkmark
                                label
                                    input(type='radio' name='brand' value="name" data-filter="Trancend")/ Trancend
                                    span.checkmark
                                label
                                    input(type='radio' name='brand' value="name" data-filter="Kingston")/ Kingston
                                    span.checkmark
                                label
                                    input(type='radio' name='brand' value="name" data-filter="Intel")/ Intel
                                    span.checkmark
                                label
                                    input(type='radio' name='brand' value="name" data-filter="Western Digital")/ Western Digital
                                    span.checkmark
                        .filter-by-size-standard.filter-prd
                            .title-standard
                                h4 Chuẩn ổ cứng
                            .form-check
                                label
                                    input(type='radio' name="standard" value="standard" data-filter="2.5inch")/ 2.5 inch
                                    span.checkmark
                                label
                                    input(type='radio' name="standard" value="standard" data-filter="mSata")/ mSata
                                    span.checkmark
                                label
                                    input(type='radio' name="standard" value="standard" data-filter="M.2 Sata")/ M.2 Sata
                                    span.checkmark
                                label
                                    input(type='radio' name="standard" value="standard" data-filter="M.2 PCIe")/ M.2 PCIe
                                    span.checkmark
                                
                        .filter-by-price.filter-prd
                            .title-price
                                h4 Giá sản phẩm
                            .form-check
                                label
                                    input(type='radio' name="price" value="price" data-filter=[500000,1000000])/ 500.000 - 1.000.000
                                    span.checkmark
                                label
                                    input(type='radio' name="price" value="price" data-filter=[1000000,1500000])/ 1.000.000 - 1.500.000
                                    span.checkmark
                                label
                                    input(type='radio' name="price" value="price" data-filter=[1500000,2000000])/ 1.500.000 - 2.000.000
                                    span.checkmark
                                label
                                    input(type='radio' name="price" value="price" data-filter=[2000000,3000000])/ 2.000.000 - 3.000.000
                                    span.checkmark
                                label
                                    input(type='radio' name="price" value="price" data-filter=[3000000,5000000])/ 3.000.000 - 5.000.000
                                    span.checkmark
                                label
                                    input(type='radio' name="price" value="price" data-filter=[5000000,10000000])/ 5.000.000 - 10.000.000
                                    span.checkmark
                        .filter-by-capacity.filter-prd
                            .title-capacity
                                h4 Dung lượng
                            .form-check
                                label
                                    input(type='radio' name="memory" value="memory" data-filter=[120, 128])/ 120 -128GB
                                    span.checkmark
                                label
                                    input(type='radio' name="memory" value="memory" data-filter=[240, 256])/ 240 - 256GB
                                    span.checkmark
                                label
                                    input(type='radio' name="memory" value="memory" data-filter=[480, 512])/ 480 - 512GB
                                    span.checkmark
                                label
                                    input(type='radio' name="memory" value="memory" data-filter=[960, 1000])/ 960GB - 1TB
                                    span.checkmark
                               
                        
                .col-md-9
                    .product-list-filtered
                        .title-top-change-layout-prd
                            .title-product
                                h5 Ổ cứng SSD
                            .layout-and-sort
                                .list-icon 
                                    img(src="./../images/product-list/list-disactive.svg", alt="list-icon")
                                .grid-icon
                                    img(src="./../images/product-list/grid-active.svg", alt="grid-icon")
                                .sort-product-by-brand
                                    .dropdown#prd-l1
                                        button#dropdownMenuButton.btn.btn-secondary.dropdown-toggle(type='button' data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                                            | Mặc định
                                        .dropdown-menu.prod-l1(aria-labelledby='dropdownMenuButton')
                                            a.dropdown-item(href='#' data-sort-by= "all" data-filter="all") Mặc định
                                            a.dropdown-item(href='#' data-sort-by="name" data-filter="ASC") A - Z
                                            a.dropdown-item(href='#' data-sort-by="name" data-filter="DESC") Z - A
                                            a.dropdown-item(href='#' data-sort-by="price" data-filter="ASC") Giá tăng dần
                                            a.dropdown-item(href='#' data-sort-by="price" data-filter="DESC") Giá giảm dần
