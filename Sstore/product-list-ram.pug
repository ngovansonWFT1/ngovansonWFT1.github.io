            .row
                .col-lg-3
                    .filter-product

                        .filter-by-brand.filter-prd
                            .title-brand
                                h4 Thương hiệu
                            .form-check
                                label
                                    input(type='radio' name='brand' value="name" data-filter="Apacer")/ Apacer
                                    span.checkmark
                                label
                                    input(type='radio' name='brand' value="name" data-filter="Kingston")/ Kingston
                                    span.checkmark
                                label
                                    input(type='radio' name='brand' value="name" data-filter="G.Skill")/ G.Skill
                                    span.checkmark
                                label
                                    input(type='radio' name='brand' value="name" data-filter="Corsair")/ Corsair
                                    span.checkmark
                               
                        .filter-by-size-standard.filter-prd
                            .title-standard
                                h4 Bus Ram
                            .form-check
                                label
                                    input(type='radio' name="standard" value="standard" data-filter=1333)/ 1333MHz
                                    span.checkmark
                                label
                                    input(type='radio' name="standard" value="standard" data-filter=1600)/ 1600MHz
                                    span.checkmark
                                label
                                    input(type='radio' name="standard" value="standard" data-filter=2133)/ 2133MHz
                                    span.checkmark
                                label
                                    input(type='radio' name="standard" value="standard" data-filter=2400)/ 2400MHz
                                    span.checkmark
                                label
                                    input(type='radio' name="standard" value="standard" data-filter=2666)/ 2666Mhz
                                    span.checkmark
                                
                        .filter-by-price.filter-prd
                            .title-price
                                h4 Giá sản phẩm
                            .form-check
                                label
                                    input(type='radio' name="price" value="price" data-filter=[200000,500000])/ 200.000 - 500.000
                                    span.checkmark
                                label
                                    input(type='radio' name="price" value="price" data-filter=[500000,1000000])/ 500.000 - 1.000.000
                                    span.checkmark
                                label
                                    input(type='radio' name="price" value="price" data-filter=[1000000,2000000])/ 1.000.000 - 2.000.000
                                    span.checkmark
                                label
                                    input(type='radio' name="price" value="price" data-filter=[2000000,3000000])/ 2.000.000 - 3.000.000
                                    span.checkmark
                                //- label
                                //-     input(type='radio' name="price" value="price" data-filter=[3000000,5000000])/ 3.000.000 - 5.000.000
                                //- label
                                //-     input(type='radio' name="price" value="price" data-filter=[5000000,10000000])/ 5.000.000 - 10.000.000
                        .filter-by-capacity.filter-prd
                            .title-capacity
                                h4 Dung lượng
                            .form-check
                                label
                                    input(type='radio' name="memory" value="memory" data-filter=[0, 4])/ 4GB
                                    span.checkmark
                                label
                                    input(type='radio' name="memory" value="memory" data-filter=[5, 8])/ 8GB
                                    span.checkmark
                                label
                                    input(type='radio' name="memory" value="memory" data-filter=[9, 16])/ 16GB
                                    span.checkmark
                                label
                                    input(type='radio' name="memory" value="memory" data-filter=[17, 32])/ 32GB
                                    span.checkmark
                               
                        
